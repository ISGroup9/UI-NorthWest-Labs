<UserControl x:Class="UI_NorthWest_Labs2.Pages.Catalogs.ProtocolNotebook"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">

        <Button x:Name="UploadProtocol" Content="Upload" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="UploadProtocol_Click" Margin="30,89,0,141" />

        <Button x:Name="DownloadProtocol" Content="Download" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="DownloadProtocol_Click" Margin="158,89,0,141" />

    </Grid>
</UserControl>
