<UserControl x:Class="UI_NorthWest_Labs2.Pages.Catalogs.ProtocolNotebook"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <TextBlock HorizontalAlignment="Left" Style ="{StaticResource Heading2}" TextWrapping="Wrap" Text="Notebook Options" VerticalAlignment="Top" Margin="0,-1,0,0" Width="268"/>

        <Button x:Name="UploadProtocol" Content="Upload" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Click="UploadProtocol_Click" Margin="22,37,0,0" />

        <Button x:Name="DownloadProtocol" Content="Download" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Click="DownloadProtocol_Click" Margin="150,37,0,0" />

    </Grid>
</UserControl>
