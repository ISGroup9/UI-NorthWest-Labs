<UserControl x:Class="UI_NorthWest_Labs2.Pages.Catalogs.PharmacologyServicesCatalog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer/>
        <TextBlock HorizontalAlignment="Left" Style ="{StaticResource Heading2}" TextWrapping="Wrap" Text="Catalog Options" VerticalAlignment="Top" Margin="0,-1,0,0" Width="268"/>
        <Button x:Name="UploadCatalog" Content="Upload" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Click="UploadCatalog_Click" Margin="23,37,0,0" />
        <Button x:Name="DownloadCatalog" Content="Download" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Click="DownloadCatalog_Click" Margin="148,37,0,0" />

    </Grid>
</UserControl>
