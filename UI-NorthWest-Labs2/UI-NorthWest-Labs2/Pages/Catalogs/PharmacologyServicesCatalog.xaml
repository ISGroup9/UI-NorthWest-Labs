<UserControl x:Class="UI_NorthWest_Labs2.Pages.Catalogs.PharmacologyServicesCatalog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer/>
        <Button x:Name="UploadCatalog" Content="Upload" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="UploadCatalog_Click" Margin="30,89,0,141" />
        <Button x:Name="DownloadCatalog" Content="Download" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="DownloadCatalog_Click" Margin="158,89,0,141" />

    </Grid>
</UserControl>
