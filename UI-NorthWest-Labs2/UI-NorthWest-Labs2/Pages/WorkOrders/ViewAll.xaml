<UserControl x:Class="UI_NorthWest_Labs2.Pages.WorkOrders.ViewAll"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
        <mui:ModernTab SelectedSource="/Pages/WorkOrders/WorkOrdersPendingReceipt.xaml#1" Layout="Tab">
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="Pending Receipt" Source="/Pages/WorkOrders/WorkOrdersPendingReceipt.xaml#1"/>
                <mui:Link DisplayName="In Process" Source="/Pages/WorkOrders/WorkOrdersPendingReceipt.xaml#2" />
                <mui:Link DisplayName="Rework Needed" Source="/Pages/WorkOrders/WorkOrdersPendingReceipt.xaml#3"/>
                <mui:Link DisplayName="Awaiting Approval" Source="/Pages/WorkOrders/WorkOrdersPendingReceipt.xaml#4"/>
                <mui:Link DisplayName="Completed" Source="/Pages/WorkOrders/Completed/CompletedWorkOrders.xaml#5" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
